{
  "name": "@switchboard-xyz/sui-sdk",
  "version": "0.1.9",
  "description": "API wrapper for intergating with the Switchboard program on our Sui implementation",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/esm/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "files": [
    "dist",
    "src",
    "package.json",
    "package-lock.json"
  ],
  "scripts": {
    "prebuild": "pnpm lint",
    "build": "pnpm exec shx rm -rf dist || true; pnpm exec tsc && pnpm exec tsc -p tsconfig.cjs.json && echo '{\"type\":\"commonjs\"}' > dist/cjs/package.json",
    "check-types": "tsc --noEmit",
    "docgen": "typedoc --out ./docs/ ./src",
    "docgen:deploy": "pnpm docgen && firebase deploy --project switchboard-docs --only hosting:switchboard-sui-sdk",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "prepack": "pnpm build",
    "test": "pnpm exec jest --passWithNoTests"
  },
  "dependencies": {
    "@isaacs/ttlcache": "^1.4.1",
    "@mysten/sui": "^1",
    "@switchboard-xyz/common": "^4.1.10"
  },
  "devDependencies": {
    "eslint-plugin-simple-import-sort": "^12.1.1"
  },
  "engines": {
    "node": ">=12.0.0"
  }
}